<% if document.manifest %>
  <iframe
      src="<%= mirador_index_path(
        manifest: document.manifest
      ) %>"
      allowfullscreen="true"
      class="mirador-embed-wrapper"
      frameborder="0"
      marginwidth="0"
      marginheight="0"
      scrolling="no"
      width="100%"
    ></iframe>
<% end %>
